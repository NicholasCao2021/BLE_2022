<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>bluetooth_cpp_node</name>
  <version>0.0.0</version>
  <description>CPP ROS2 node template</description>
  <maintainer email="ak21472@bristol.ac.uk">root</maintainer>
  <license>MIT License @ NicholasCao 2022</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>std_srvs</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>
  <depend>tf2_geometry_msgs</depend>
  <depend>std_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>trajectory_msgs</depend>
  <depend>mavros_msgs</depend>
  <depend>mavros_extras</depend>

  <!-- List custom message dependencies here -->
  <!-- <depend>ros2_msgs_template</depend> -->

  <depend>bluetooth_msgs</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
